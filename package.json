{
  "name": "@dev-wizard/engine",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./loader/*": {
      "types": "./dist/loader/*.d.ts",
      "import": "./dist/loader/*.js",
      "default": "./dist/loader/*.js"
    },
    "./loader/*.js": {
      "types": "./dist/loader/*.d.ts",
      "import": "./dist/loader/*.js",
      "default": "./dist/loader/*.js"
    },
    "./programmatic/*": {
      "types": "./dist/programmatic/*.d.ts",
      "import": "./dist/programmatic/*.js",
      "default": "./dist/programmatic/*.js"
    },
    "./programmatic/*.js": {
      "types": "./dist/programmatic/*.d.ts",
      "import": "./dist/programmatic/*.js",
      "default": "./dist/programmatic/*.js"
    },
    "./runtime/*": {
      "types": "./dist/runtime/*.d.ts",
      "import": "./dist/runtime/*.js",
      "default": "./dist/runtime/*.js"
    },
    "./runtime/*.js": {
      "types": "./dist/runtime/*.d.ts",
      "import": "./dist/runtime/*.js",
      "default": "./dist/runtime/*.js"
    },
    "./runtime/telemetry/*": {
      "types": "./dist/runtime/telemetry/*.d.ts",
      "import": "./dist/runtime/telemetry/*.js",
      "default": "./dist/runtime/telemetry/*.js"
    },
    "./runtime/telemetry/*.js": {
      "types": "./dist/runtime/telemetry/*.d.ts",
      "import": "./dist/runtime/telemetry/*.js",
      "default": "./dist/runtime/telemetry/*.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist src/**/*.js src/**/*.js.map src/**/*.d.ts",
    "lint": "eslint src --ext .ts --max-warnings=0",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.test.json"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "execa": "^9.6.1",
    "fast-glob": "^3.3.3",
    "handlebars": "^4.7.8",
    "json5": "^2.2.3",
    "yaml": "^2.8.2",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
